{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    
  },
  "rules": {
    "whitespace": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "member-access": [
      false
    ],
    "interface-name": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "quotemark": [
      "single"
    ],
    "trailing-comma": [
      false
    ],
    "no-empty": false,
    "max-classes-per-file": [
      7
    ],
    "max-line-length": [
      180
    ]
  },
  "rulesDirectory": [
  ]
}